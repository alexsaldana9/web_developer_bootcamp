<%- include ("partials/header") %>

<h1>Posts</h1>

<ul>
<% for(var i = 0; i <posts.length; i++) { %>
	<li> <%= posts[i].title %>  - <%= posts[i].author %></li>
<% } %>
</ul>

<h2>Using a forEach loop</h2>

<ul>
<% posts.forEach(function(post) { %>
	<li> <%= post.title %>  - <strong><%= post.author %></strong></li>
<% }) %>
</ul>


